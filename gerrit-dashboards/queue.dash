[dashboard]
title = Queue
description =
foreach = is:open (is:watched OR is:starred) -owner:self -label:Code-Review<=+2,self  label:Verified+1,jenkins label:Workflow+0

[section "New"]
query = -label:Code-Review<=2

[section "+1"]
query = -label:Code-Review<=-1 label:Code-Review+1 -label:Code-Review+2

[section "+2"]
query = -label:Code-Review<=-1 label:Code-Review+2
