#!/usr/bin/env python

import string
import uuid


ALPHABET = string.digits + string.letters


def encode(number):
    """Converts an integer to a string."""
    s = ''
    while number != 0:
        number, i = divmod(number, len(ALPHABET))
        s = ALPHABET[i] + s
    return s


s = encode(uuid.uuid4().int)
print s, len(s)
